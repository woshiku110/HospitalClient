<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fbfbfb">
    <include layout="@layout/item_rece_address"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="4dp"
        android:background="@color/titleBlue"
        />
    <LinearLayout
        android:id="@+id/item_prescrible_wait_pay_contain"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:background="@color/check_detail_bg">

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/item_prescrible_wait_pay_qos_txt"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="对该订单有疑问?"
            android:textColor="@color/titleBlue"
            android:gravity="center_horizontal"
            android:textSize="12sp"
            android:padding="5dp"/>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/item_prescrible_parent_line"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:background="#e2e2e2">
        <LinearLayout
            android:id="@+id/item_prescrible_child_line"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@android:color/white"
            android:layout_marginTop="1px"
            android:layout_marginBottom="1px"
            android:orientation="vertical">

        </LinearLayout>
    </LinearLayout>
</LinearLayout>